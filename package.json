{
  "name": "rest-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack && node -r babel-register index.js",
    "dev": "nodemon",
    "watch": "webpack --watch",
    "start-api": "json-server ./api.js",
    "delay-api": "json-server ./api.js --delay 10000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Decoen/rest-api-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julien-Decoen/rest-api-test/issues"
  },
  "homepage": "https://github.com/Julien-Decoen/rest-api-test#readme",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "json-server": "^0.14.0",
    "node-sass": "^4.10.0",
    "nunjucks": "^3.1.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.23.1",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.6",
    "webpack-cli": "^3.1.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git/*",
      "node_modules/*",
      ".public/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node -r babel-register index.js"
    },
    "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
      "app/*",
      "front/*"
    ],
    "ext": "js json html css"
  }
}
